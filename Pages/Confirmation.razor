
<div style="max-width:300px;">
    <div>@text</div>
    <btn class="btn btn-primary"  @onclick='()=>UserClicked("OK")'>OK</btn>
    <btn class="btn btn-primary" @onclick='()=>UserClicked("Cancel")'>Cnacel</btn>
</div>
@code {
    [Parameter]
    public string text { get; set; }
    [Parameter]
    public Action<string> UserClicked { get; set; }
}
